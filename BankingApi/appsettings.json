{
  "AuthServer": {
    "Authority": "https://localhost:7020/",
    "Audience": "banking-api",
    "RequireHttpsMetadata": true,
    "ValidateAudience": true,
    "ClockSkewSeconds": 60
  },
  "ConnectionStrings": {
    "BankingApi": "BankingDb.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
      "BankingApi._2_Modules.Owners": "Debug",
      "BankingApi._2_Modules.Core": "Debug",
      "BankingApi._3_Infrastructure.Database": "Debug"
    }
  },
  "AllowedHosts": "*"
}
